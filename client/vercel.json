{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      },
      {
        "src": "/manifest.json",
        "dest": "/public/manifest.json",
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate"
        }
      }
    ]
  }
  